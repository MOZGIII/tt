{
  "name": "tt-scripts",
  "private": true,
  "scripts": {
    "prettier:check": "cd \"$INIT_CWD\" && prettier --list-different **/*",
    "prettier:fix": "cd \"$INIT_CWD\" && prettier --write **/*"
  },
  "dependencies": {
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@types/prettier": "^2"
  }
}
