{
  "name": "tt-scripts",
  "private": true,
  "scripts": {
    "scripts:env": "env",
    "prettier:check": "cd ../.. && prettier --ignore-path .prettierignore --list-different \"$INIT_CWD/**/*\"",
    "prettier:fix": "cd ../.. && prettier --ignore-path .prettierignore --write \"$INIT_CWD/**/*\""
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  }
}
